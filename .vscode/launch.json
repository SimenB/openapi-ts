{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "Attach",
      "port": 9229,
      "skipFiles": ["<node_internals>/**"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "openapi-ts",
      "skipFiles": ["<node_internals>/**"],
      "runtimeExecutable": "node",
      "runtimeArgs": ["-r", "ts-node/register/transpile-only"],
      "program": "${workspaceFolder}/packages/openapi-ts/src/cli.ts",
      "args": ["-f", "${workspaceFolder}/dev/openapi-ts.config.ts"]
    }
  ]
}
